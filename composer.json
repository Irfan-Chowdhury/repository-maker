{
    "name": "irfan-chowdhury/repository-maker",
    "description": "This package will create your necessary files for repository design pattern environment.",
    "keywords": [
        "repository",
        "repository-design-pattern",
        "laravel-repository",
        "laravel-repository-design-pattern"
    ],
    "type": "library",
    "homepage": "https://github.com/Irfan-Chowdhury/repository-maker",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Irfan\\RepositoryMaker\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Irfan Chowdhury",
            "email": "irfanchowdhury80@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4",
        "laravel/framework": ">=8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Irfan\\RepositoryMaker\\RepositoryServiceProvider"
            ]
        }
    }
}
